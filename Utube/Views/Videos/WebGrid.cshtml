@model IEnumerable<Utube.Models.Video>

@{
    ViewBag.Title = "WebGrid";
    var grid = new WebGrid(source: Model, rowsPerPage: 3,  columnNames: new[] { "Info", "Videotitle","Vlink","Datein" });
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>WebGrid</h2>

@grid.GetHtml(tableStyle: "webgrid-table",  
   headerStyle: "webgrid-header",  
   footerStyle: "webgrid-footer",  
   alternatingRowStyle: "webgrid-alternating-row",  
   selectedRowStyle: "webgrid-selected-row",  
    rowStyle: "webgrid-row-style", 
columns: grid.Columns(grid.Column(columnName: "Videotitle", header: "Название"),
grid.Column(columnName: "Vlink", header: "Ссылка"),
grid.Column(columnName: "Info", header: "Описание"),
grid.Column(columnName: "Datein", header: "Дата")));
